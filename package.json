{
  "name": "@myprototypewhat/mcp-auto-install",
  "version": "1.0.0",
  "description": "Automatic management and installation tool for Model Context Protocol (MCP) servers",
  "main": "build/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc && node --input-type=module -e \"import { chmodSync } from 'fs'; chmodSync('build/index.js', '755');\"",
    "start": "node build/index.js",
    "dev": "tsc -w",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "automation",
    "installation",
    "management",
    "cli"
  ],
  "author": "MyPrototypeWhat",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MyPrototypeWhat/mcp-auto-install.git"
  },
  "bugs": {
    "url": "https://github.com/MyPrototypeWhat/mcp-auto-install/issues"
  },
  "homepage": "https://github.com/MyPrototypeWhat/mcp-auto-install#readme",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.7.0",
    "@types/node": "^22.13.10",
    "commander": "^12.0.0",
    "npx-scope-finder": "^1.3.0",
    "typescript": "^5.8.2",
    "zod": "^3.24.2"
  },
  "bin": {
    "mcp-auto-install": "./build/index.js"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.6"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
